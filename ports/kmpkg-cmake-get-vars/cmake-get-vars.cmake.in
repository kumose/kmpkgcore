# Set KMPKG_TRACE_CMAKE_GET_VARS to trace file path and call stack on every load.
if(KMPKG_TRACE_CMAKE_GET_VARS)
    message(WARNING "Loading CMake variables from ${CMAKE_CURRENT_LIST_FILE}")
elseif(NOT Z_KMPKG_CMAKE_GET_VARS_FILE_LAST_LOADED STREQUAL "${CMAKE_CURRENT_LIST_FILE}")
    set(Z_KMPKG_CMAKE_GET_VARS_FILE_LAST_LOADED "${CMAKE_CURRENT_LIST_FILE}" CACHE INTERNAL "")
    message(STATUS "Loading CMake variables from ${CMAKE_CURRENT_LIST_FILE}")
endif()

if("@KMPKG_BUILD_TYPE@" STREQUAL "" OR "@KMPKG_BUILD_TYPE@" STREQUAL "release")
    include("${CMAKE_CURRENT_LIST_DIR}/cmake-get-vars@configuration_suffix@-@TARGET_TRIPLET@-rel.cmake.log")
endif()
if("@KMPKG_BUILD_TYPE@" STREQUAL "" OR "@KMPKG_BUILD_TYPE@" STREQUAL "debug")
    include("${CMAKE_CURRENT_LIST_DIR}/cmake-get-vars@configuration_suffix@-@TARGET_TRIPLET@-dbg.cmake.log")
endif()
